config smartdns
	option port '6053'
	option tcp_server '1'
	option ipv6_server '0'
	option dualstack_ip_selection '0'
	option prefetch_domain '1'
	option serve_expired '1'
	option redirect 'none'
	option rr_ttl_min '300'
	option seconddns_enabled '1'
	option seconddns_port '5335'
	option seconddns_tcp_server '1'
	option seconddns_server_group 'us'
	option seconddns_no_speed_check '0'
	option seconddns_no_rule_addr '1'
	option seconddns_no_rule_nameserver '1'
	option seconddns_no_rule_ipset '1'
	option seconddns_no_rule_soa '0'
	option seconddns_no_dualstack_selection '1'
	option seconddns_no_cache '0'
	option force_aaaa_soa '1'
	option coredump '0'
	option enabled '1'
	option server_name 'cn'
	list old_redirect 'none'
	list old_port '6053'
	list old_enabled '1'

